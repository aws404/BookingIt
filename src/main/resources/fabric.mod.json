{
  "schemaVersion": 1,
  "id": "booking_it",
  "version": "${version}",

  "name": "Booking It",
  "description": "A library for adding to the Minecraft recipe book.",
  "authors": [
    "aws404"
  ],
  "contact": {
    "sources": "https://github.com/aws404/BookingIt"
  },

  "license": "CC0-1.0",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.github.aws404.booking_it.BookingIt"
    ],
    "client": [
      "com.github.aws404.booking_it.BookingIt"
    ],
    "mm:early_risers": [
      "com.github.aws404.booking_it.EarlyRiser"
    ]
  },
  "mixins": [
    "booking_it.mixins.json"
  ],
  "accessWidener": "booking_it.accesswidener",

  "depends": {
    "fabricloader": ">=0.12.12",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17",
    "mm": "*"
  }
}
